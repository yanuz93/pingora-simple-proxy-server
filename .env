# Tracing
ENABLE_TRACING=true
DD_AGENT_HOST=localhost:8126
SERVICE_NAME=enterprise-proxy

# Kubernetes
ENABLE_KUBERNETES=true
KUBERNETES_NAMESPACE=default
KUBERNETES_SERVICE_LABEL_SELECTOR=app=service

# Caching
ENABLE_CACHING=true
MAX_CACHE_SIZE=1000
CACHE_TTL_SECONDS=300

# A/B Testing (configured via config file or env)
ENABLE_AB_TESTING=true

# RUST_LOG
RUST_LOG=info,pingora=debug

# Eureka
EUREKA_URLS=http://eureka1:8761,http://eureka2:8761